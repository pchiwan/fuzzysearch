{
  "name": "fuzzyhighlight",
  "description": "Tiny, fast, and dependency-less fuzzy search + highlighting in JavaScript",
  "version": "4.0.0",
  "homepage": "https://github.com/pchiwan/fuzzysearch",
  "author": {
    "name": "SÃ­lvia Mur Blanch",
    "email": "silvia.murblanch@gmail.com",
